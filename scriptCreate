create table menu_item (menu_id integer not null, item_id integer not null, primary key (menu_id, item_id)) engine=InnoDB;
create table Ticket (numero_transaction integer not null auto_increment, ID_COMMANDE integer, primary key (numero_transaction)) engine=InnoDB;
alter table commande_item add constraint FK7j7gckwbfixewrj7xqdlc4rwm foreign key (item_id) references Item (Id);
alter table commande_item add constraint FKl7ssvrvon24etdrx2lj5iukod foreign key (commande_id) references Commande (Id);
alter table Facture add constraint FKaljwyno893lkm0lk8ufxbft2g foreign key (ID_COMMANDE) references Commande (Id);
alter table menu_item add constraint FK796mqp7d732udd1dxq7289hxv foreign key (item_id) references Item (Id);
alter table menu_item add constraint FKkh7jdsk1a1lrm72xuirjaggbv foreign key (menu_id) references Item (Id);
alter table Ticket add constraint FK6613kwa8jd2v15gw2o45vpynh foreign key (ID_COMMANDE) references Commande (Id);
