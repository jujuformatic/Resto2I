alter table commande_item add constraint FKl7ssvrvon24etdrx2lj5iukod foreign key (commande_id) references Commande (Id);
alter table Facture add constraint FKaljwyno893lkm0lk8ufxbft2g foreign key (ID_COMMANDE) references Commande (Id);
alter table menu_item add constraint FK796mqp7d732udd1dxq7289hxv foreign key (item_id) references Item (id);
alter table menu_item add constraint FKkh7jdsk1a1lrm72xuirjaggbv foreign key (menu_id) references Item (id);
alter table Ticket add constraint FK6613kwa8jd2v15gw2o45vpynh foreign key (ID_COMMANDE) references Commande (Id);
create table Commande (Id integer not null auto_increment, commentaire varchar(255), horaire TIMESTAMP not null, prix_total NUMERIC(7,2) not null, valide bit, no_table integer, primary key (Id)) engine=InnoDB;
create table commande_item (commande_id integer not null, item_id integer not null, primary key (commande_id, item_id)) engine=InnoDB;
create table Facture (numero_facture integer not null auto_increment, conditions_paiement varchar(255), coordonnees_client varchar(255), nom_client varchar(255), numero_TVA_client bigint, ID_COMMANDE integer, primary key (numero_facture)) engine=InnoDB;
create table Item (item_type varchar(31) not null, id integer not null auto_increment, TVA NUMERIC(3,1) not null, categorie varchar(255) not null, hidden bit not null, nom varchar(255) not null, prix NUMERIC(7,2) not null, primary key (id)) engine=InnoDB;
create table menu_item (menu_id integer not null, item_id integer not null, primary key (menu_id, item_id)) engine=InnoDB;
create table Tables (num_table integer not null auto_increment, nbPersonnes integer, occupe bit, primary key (num_table)) engine=InnoDB;
create table Ticket (numero_transaction integer not null auto_increment, ID_COMMANDE integer, primary key (numero_transaction)) engine=InnoDB;
