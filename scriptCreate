create table Commande (Id integer not null auto_increment, commentaire varchar(255), horaire TIMESTAMP not null, prix_total NUMERIC(7,2) not null, table integer, primary key (Id)) engine=InnoDB;
create table Facture (conditions_paiement varchar(255), coordonnees_client varchar(255), nom_client varchar(255), numero_TVA_client bigint, commande_Id integer not null, primary key (commande_Id)) engine=InnoDB;
create table Item (Id integer not null auto_increment, TVA NUMERIC(3,1) not null, categorie varchar(255) not null, nom varchar(255) not null, prix NUMERIC(7,2) not null, primary key (Id)) engine=InnoDB;
create table Menu (Id integer not null auto_increment, primary key (Id)) engine=InnoDB;
create table menu_item (menu_id integer not null, item_id integer not null, primary key (menu_id, item_id)) engine=InnoDB;
create table Ticket (commande_Id integer not null, primary key (commande_Id)) engine=InnoDB;
alter table Facture add constraint FKlm7lh52lqmr7jy3pupuo9966g foreign key (commande_Id) references Commande (Id);
alter table menu_item add constraint FK796mqp7d732udd1dxq7289hxv foreign key (item_id) references Item (Id);
alter table menu_item add constraint FKq4q5o9o68narna0o6do19dw48 foreign key (menu_id) references Menu (Id);
alter table Ticket add constraint FKovoc9wrl0d3vm3qpktkl7q1jx foreign key (commande_Id) references Commande (Id);
